
func (s *{{.Model.Name}}) Insert(tx dblib.DBLike) error {
		stmt, err := tx.Prepare("{{.Model.InsertStatement}}")
		if err != nil {
			return err
		}

		result, err := stmt.Exec({{ .Model.FieldString("s.", m.FieldSliceWithoutID(), "")}}) // ADD FIELD HERE
		if err != nil {
			return err
    }

    s.ID, err = result.LastInsertId()
    if err != nil {
      return err
    }
	  return nil
}

func (s *{{.Model.Name}}) Delete(tx dblib.DBLike) error {
		stmt, err := tx.Prepare("DELETE FROM {{.Model.Name}} WHERE ID = ?")
		if err != nil {
			return err
		}

		_, err = stmt.Exec(s.ID)
		if err != nil {
			return err
    }

	  return nil
}

func Create{{.Model.Name}}Table(tx dblib.DBLike) error {
		stmt, err := tx.Prepare(`{{.Model.CreateTable()}}`)
		if err != nil {
			return err
		}

		_, err = stmt.Exec()
		if err != nil {
			return err
		}
	  return nil
}

